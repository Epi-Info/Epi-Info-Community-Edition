<UserControl
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing" x:Class="Epi.Windows.Menu.MainWindow"
       xmlns:Controls="Epi.Windows.Menu.MainWindow"
        Height="560" MaxHeight="570" Width="690" MaxWidth="690">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Simple Styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <Storyboard x:Key="sbShowRightMenu" >
                <!--<ThicknessAnimation Storyboard.TargetProperty="Margin" From="0,-340,0,0" To="0,-340,0,0" DecelerationRatio=".9" Duration="0:0:1" />-->
                <!--<DoubleAnimation Storyboard.TargetProperty="Opacity" From="0.0" To="5.0"  Duration="0:0:0.5" />-->
                <DoubleAnimation Storyboard.TargetName="pnlRightMenu" Storyboard.TargetProperty="Height" From="0" To="200"  Duration="0:0:0.1" />
            </Storyboard>
            <Storyboard x:Key="sbHideRightMenu">
                <DoubleAnimation Storyboard.TargetName="pnlRightMenu" Storyboard.TargetProperty="Height" From="200" To="0"  Duration="0:0:0.2" />
                <!--<ThicknessAnimation Storyboard.TargetProperty="Margin" From="0,-340,0,0" To="0,-340,0,0" AccelerationRatio=".9" Duration="0:0:1" />-->
                <!--<DoubleAnimation Storyboard.TargetProperty="Opacity" From="1.0" To="0.0"  Duration="0:0:0.2" />-->
            </Storyboard>

            <Storyboard x:Key="showEpiResource">
                <DoubleAnimation Storyboard.TargetName="submenuEpiResouces" Storyboard.TargetProperty="Height" From="0" To="50"  Duration="0:0:0.1" />
                <DoubleAnimation Storyboard.TargetName="pnlRightMenu" Storyboard.TargetProperty="Height" From="200" To="240"  Duration="0:0:0.1" />
            </Storyboard>
            <Storyboard x:Key="hideEpiResource">
                <DoubleAnimation Storyboard.TargetName="pnlRightMenu" Storyboard.TargetProperty="Height" From="240" To="200"  Duration="0:0:0.2" />
                <DoubleAnimation Storyboard.TargetName="submenuEpiResouces" Storyboard.TargetProperty="Height" From="50" To="0"  Duration="0:0:0.2" />
            </Storyboard>

        </ResourceDictionary>



    </UserControl.Resources>
    <Grid x:Name="grdMain" Background="White">
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="0" Panel.ZIndex="2" DockPanel.Dock="Top" Name="pnlRightMenu" Orientation="Horizontal" Height="0" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,70,15,0" Background="Transparent">
            <!--<Grid>
                <Button Name="btnRightMenuHide" Width="48" Height="48" Click="btnRightMenuHide_Click" Content="Hide" Visibility="Hidden"/>
                <Button Name="btnRightMenuShow" Width="48" Height="48" Click="btnRightMenuShow_Click" Content="Show"/>
            </Grid>-->
            <Border BorderBrush="#5c5d5f" BorderThickness="1" Width="170" Height="Auto" Background="White"  >
                <WrapPanel Orientation="Vertical">
                    <ContentControl MouseDown="contents_Click">
                        <StackPanel>
                            <TextBlock Style="{StaticResource setting_buttons}">CONTENTS</TextBlock>
                        </StackPanel>
                    </ContentControl>
                    <ContentControl MouseDown="howToVideo_Click">
                        <StackPanel>
                            <TextBlock Style="{StaticResource setting_buttons}">HOW TO' VIDEOS</TextBlock>
                        </StackPanel>
                    </ContentControl>
                    <ContentControl MouseDown="epiInfoQA_Click">
                        <StackPanel>
                            <TextBlock Style="{StaticResource setting_buttons}">EPI INFO Q&amp;A</TextBlock>
                        </StackPanel>
                    </ContentControl>
                    <ContentControl MouseDown="contactHelpDesk_Click">
                        <StackPanel>
                            <TextBlock Style="{StaticResource setting_buttons}">CONTACT HELP DESK</TextBlock>
                        </StackPanel>
                    </ContentControl>
                    <ContentControl MouseDown="Other_EpiResource_Click">
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Style="{StaticResource setting_buttons}">OTHER EPI RESOURCES
                                            <Image HorizontalAlignment="Left"  RenderOptions.BitmapScalingMode="HighQuality" Width="11" Margin="5 0 0 -1">
                                                <Image.Source>
                                                    <BitmapImage UriSource="images/showandhide_arrow.png"/>
                                                </Image.Source>
                                            </Image>
                            </TextBlock>

                        </StackPanel>
                    </ContentControl>
                    <StackPanel x:Name="submenuEpiResouces" Height="0">
                        <ContentControl MouseDown="activEpi_Click">
                            <StackPanel Background="#e6e7e8">
                                <TextBlock Style="{StaticResource setting_buttons}" Margin="10 0 0 0">ACTIVEEPI.com</TextBlock>
                            </StackPanel>
                        </ContentControl>
                        <ContentControl MouseDown="openEpi_Click">
                            <StackPanel Background="#e6e7e8" >
                                <TextBlock Style="{StaticResource setting_buttons}" Margin="10 0 0 0">OPENEPI.com</TextBlock>
                            </StackPanel>
                        </ContentControl>
                    </StackPanel>
                    <ContentControl MouseDown="epi_InfoLogs_Click">
                        <StackPanel>
                            <TextBlock Style="{StaticResource setting_buttons}">EPI INFO LOGS</TextBlock>
                        </StackPanel>
                    </ContentControl>
                    <ContentControl MouseDown="options_Click">
                        <StackPanel>
                            <Border BorderBrush="#c8c8c8" BorderThickness="0 0 0 1">
                                <TextBlock Style="{StaticResource setting_buttons}">OPTIONS</TextBlock>
                            </Border>
                        </StackPanel>
                    </ContentControl>
                    <ContentControl MouseDown="exit_Click">
                        <StackPanel>
                            <TextBlock Style="{StaticResource setting_buttons}">EXIT</TextBlock>
                        </StackPanel>
                    </ContentControl>
                </WrapPanel>
            </Border>
        </StackPanel>
