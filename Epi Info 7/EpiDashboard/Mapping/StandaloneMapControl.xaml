                            <DockPanel.ToolTip>
                                <StackPanel>
                                    <Label x:Name="tooltipLegendHeader" FontWeight="Bold">Hide/Show Legend</Label>
                                    <TextBlock x:Name="tooltipLegend"
                                               Padding="10" Background="Transparent" TextWrapping="WrapWithOverflow" Width="200">
                                        Hide or show the map legend/key of the layers on the map.
                                    </TextBlock>
                                </StackPanel>
                            </DockPanel.ToolTip>
