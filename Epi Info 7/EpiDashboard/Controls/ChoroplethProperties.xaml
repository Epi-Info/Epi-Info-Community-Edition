                                <Rectangle.ToolTip>
                                    <Label Content="Missing value color"/>
                                </Rectangle.ToolTip>
                            </Rectangle>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Margin="8,0,0,2" >
